{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "linebreak-style": ["error", "windows"],
        "no-underscore-dangle": "off",
        "no-restricted-globals": "off"
    }
}
